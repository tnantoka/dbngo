Load box.dbn

Paper 100
Set A [1 1]
Paper 10
Pen A
Set B 20
Line B 30 200 300
{
  Line 10 10 90 90
}
Pen 50
Repeat C 45 55 {
  Line 45 C 55 (C+10)
  Set [30 A] 100
}

Repeat A 0 100 {
  Repeat B 50 55 {
    Set [A B] B
  }
}

Repeat A 0 100 {
  Same? A 50 {
    Line A 0 A 100
  }
}

Repeat A 0 20 {
  Pen A
  Smaller? A 2 {
    Pen 100
  }
  NotSmaller? A 19 {
    Pen 100
  }
  NotSame? A 10 {
    Line A 0 A 100
  }
}

Box 80 90 10 20
